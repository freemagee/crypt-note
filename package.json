{
  "name": "es6-babel-react",
  "version": "2.0.0",
  "description": "Webpack transpiler",
  "main": "webpack.config.js",
  "scripts": {
    "start:dev": "webpack-dev-server --inline --hot --progress --open --config webpack.dev.js",
    "start:prod": "webpack --progress --profile --colors --config webpack.prod.js && http-server -a localhost -p 3030",
    "build:prod": "webpack --progress --profile --colors --config webpack.prod.js",
    "lint": "eslint src"
  },
