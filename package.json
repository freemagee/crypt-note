{
  "name": "es6-babel-react",
  "version": "1.0.0",
  "description": "Webpack transpiler",
  "main": "webpack.config.js",
  "scripts": {
    "start:dev": "webpack-dev-server --inline --hot --config webpack.dev.config.js --progress --open",
    "start:prod": "rimraf ./app/* && webpack --config webpack.prod.config.js --progress --profile --colors && http-server -a localhost -p 3000",
    "build:prod": "webpack --config webpack.prod.config.js --progress --profile --colors",
    "lint": "eslint src"
  },
  "author": "Neil Magee",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "html-react-parser": "^0.3.3",
    "markdown-it": "^8.3.1",
    "path": "^0.12.7",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "webpack": "^2.2.1",
    "webpack-dev-server": "2.4.2",
    "webpack-manifest-plugin": "^1.1.0"
  }
}
