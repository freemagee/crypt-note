{
  "name": "es6-babel-react",
  "version": "2.0.0",
  "description": "Webpack transpiler",
  "main": "webpack.config.js",
  "scripts": {
    "start:dev": "webpack-dev-server --inline --hot --progress --open --config webpack.dev.js",
    "start:prod": "rimraf ./app/* && webpack --config webpack.prod.config.js --progress --profile --colors && http-server -a localhost -p 3000",
    "build:prod": "rimraf ./app/* && webpack --config webpack.prod.config.js --progress --profile --colors",
    "lint": "eslint src"
  },
  "author": "Neil Magee",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "cssnano": "^3.10.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "html-react-parser": "^0.4.3",
    "html-webpack-plugin": "^3.2.0",
    "inline-manifest-webpack-plugin": "^3.0.1",
    "markdown-it": "^8.4.1",
    "node-sass": "^4.8.3",
    "path": "^0.12.7",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.4",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.20.3",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "url-loader": "^1.0.1",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3",
    "webpack-manifest-plugin": "^2.0.0",
    "webpack-merge": "^4.1.2"
  }
}
